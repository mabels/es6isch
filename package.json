  "bugs": {
    "url": "https://github.com/mabels/es6isch/issues"
  },
  "homepage": "https://github.com/mabels/es6isch#readme",
  "devDependencies": {},
  "dependencies": {
    "@babel/core": "^7.0.0-beta.40",
    "@babel/traverse": "^7.0.0-beta.40",
    "express": "^4.16.2"
  }
}
